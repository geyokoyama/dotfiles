# bare git repo alias
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'

alias l='ls -AlF'
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

alias upp='sudo /bin/sh -c "apt update && apt upgrade && apt autoremove && apt autoclean"'
alias source_bash='source $HOME/.bashrc'
alias gh='history|grep'
